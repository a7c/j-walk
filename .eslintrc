{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "fbjs",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ]
}
